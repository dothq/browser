    "url": "https://github.com/dothq/browser/issues"
  },
  "scripts": {
    "start": "cross-env ENV=dev ELECTRON_IS_DEV=1 electron --trace-warnings --trace-deprecation .",
    "build": "npm run dev:clean && concurrently -n main,renderer,web,preloads \"npm run prod:main\" \"npm run prod:renderer\" \"npm run prod:web\" \"npm run prod:preloads\"",
    "dev": "npm run dev:clean && concurrently -n main,renderer,web,preloads \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:web\" \"npm run dev:preloads\"",
    "compile": "node tools/ci.js",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps",
    "dev:clean": "node tools/download-filters.js && rimraf build",
    "dev:main": "cross-env NODE_ENV=dev START=1 webpack --config webpack.config.ts",
    "dev:renderer": "cross-env NODE_ENV=dev webpack-dev-server --config webpack.config.renderer.ts",
    "dev:web": "cross-env NODE_ENV=dev webpack-dev-server --config webpack.config.web.ts",
    "dev:preloads": "cross-env NODE_ENV=dev webpack --config webpack.config.preloads.ts",
    "prod:main": "cross-env NODE_ENV=prod webpack --config webpack.config.ts",
    "prod:renderer": "cross-env NODE_ENV=prod webpack --config webpack.config.renderer.ts",
    "prod:web": "cross-env NODE_ENV=prod webpack --config webpack.config.web.ts",
    "prod:preloads": "cross-env NODE_ENV=prod webpack --config webpack.config.preloads.ts",
    "test": "jest --coverage --passWithNoTests --coverageDirectory=output/coverage/jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "pre-commit": [
    "lint-fix"
  ],
